    },
    {
      "path": "/api/push-reminders?kind=evening&secret=tuba-secure-8821",
      "schedule": "0 16 * * *"
    },
    {
      "path": "/api/push-reminders?kind=backup&secret=tuba-secure-8821",
      "schedule": "0 18 * * *"
    },
    {
      "path": "/api/push-reminders?kind=monthly-report&secret=tuba-secure-8821",
      "schedule": "0 6 1 * *"
    },
    {
      "path": "/api/push-reminders?kind=inventory-50&secret=tuba-secure-8821",
      "schedule": "0 2 * * *"
    }
  ]
