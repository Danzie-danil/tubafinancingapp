{
  "version": 2,
  "headers": [
    {
      "source": "/tuba.mobileconfig",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-apple-aspen-config"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"tuba.mobileconfig\""
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
